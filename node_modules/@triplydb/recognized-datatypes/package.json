{
  "name": "@triplydb/recognized-datatypes",
  "version": "2.0.7",
  "dependencies": {
    "@rdfjs/types": "^1.1.0",
    "chevrotain": "^11.0.3",
    "lodash-es": "^4.17.21",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@triplydb/utils-workspace": "0.0.0",
    "@types/chai-datetime": "^0.0.38",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.8.0",
    "mocha": "^10.2.0",
    "moo": "^0.5.2"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -b tsconfig-build.json",
    "release:patch": "yarn run preversion && yarn version --immediate patch && yarn run postversion",
    "release:minor": "yarn run preversion && yarn version --immediate minor && yarn run postversion",
    "release:major": "yarn run preversion && yarn version --immediate major && yarn run postversion",
    "preversion": "../../node_modules/.bin/isCleanBranch",
    "postversion": "../../node_modules/.bin/versionToBranch recognized-datatypes",
    "test": "mocha ./lib/**/__tests__/*-test.js --require source-map-support/register",
    "util:clear": "rm -rf ./lib",
    "util:prettify": "../../node_modules/.bin/prettier --parser typescript --write $(find ./src -name '*.ts')"
  },
  "publishConfig": {
    "@triplydb:registry": " https://git.triply.cc/api/v4/projects/393/packages/npm/"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./*": {
      "import": {
        "default": "./lib/*"
      }
    }
  },
  "type": "module"
}