{
  "name": "@triplydb/speedy-memory",
  "version": "2.10.5",
  "dependencies": {
    "@triplydb/data-factory": "^3.0.2",
    "@triplydb/engine": "^2.12.4",
    "lodash-es": "^4.17.21",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@triplydb/utils-workspace": "0.0.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.2.0"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -b tsconfig-build.json",
    "test": "mocha ./lib/**/__tests__/*-test.js --require source-map-support/register",
    "util:clear": "rm -rf ./lib",
    "util:prettify": "../../node_modules/.bin/prettier --parser typescript --write $(find ./src -name '*.ts')"
  },
  "publishConfig": {
    "@triplydb:registry": "https://git.triply.cc/api/v4/projects/393/packages/npm/"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/speedy-memory.d.ts",
        "default": "./lib/speedy-memory.js"
      }
    },
    "./*": {
      "import": {
        "default": "./lib/*"
      }
    }
  },
  "type": "module"
}