{
  "name": "@triplyetl/shacl",
  "version": "1.0.8",
  "description": "Triply's SHACL and SHACL Advanced engine.",
  "main": "lib/index.js",
  "type": "module",
  "bin": {
    "validate": "lib/bin/validate.js"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "dev": "tsc -p ./tsconfig.json --watch",
    "prepare": "husky install",
    "format": "prettier 'src/**/*.ts' --write",
    "lint:check": "eslint . --ext .ts",
    "test": "mocha \"./lib/**/*.test.js\" --require source-map-support/register",
    "lint": "eslint . --fix --ext .ts",
    "preversion": "isCleanBranch",
    "postversion": "versionToBranch"
  },
  "repository": {
    "type": "git",
    "url": "https://git.triply.cc/triplyetl/shacl"
  },
  "publishConfig": {
    "@triplyetl:registry": "https://git.triply.cc/api/v4/projects/463/packages/npm/"
  },
  "keywords": [
    "shacl",
    "rdf"
  ],
  "author": "Triply",
  "license": "UNLICENSED",
  "devDependencies": {
    "@triply/utils": "^2.0.4",
    "@types/chai": "^4.3.9",
    "@types/chai-as-promised": "^7.1.7",
    "@types/lodash-es": "^4.17.10",
    "@types/mocha": "^10.0.3",
    "@types/rdfjs__term-map": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "rdf-isomorphic": "^1.3.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "lib/*.{json,js,ts,jsx,tsx}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@triplydb/data-factory": "^3.0.0",
    "@triplydb/speedy-memory": "^2.10.0",
    "@triplyetl/vocabularies": "^1.1.10",
    "@zazuko/prefixes": "^2.1.0",
    "install": "^0.13.0",
    "lodash-es": "^4.17.21",
    "shacl-engine": "^0.1.0",
    "word-wrap": "^1.2.5"
  }
}
