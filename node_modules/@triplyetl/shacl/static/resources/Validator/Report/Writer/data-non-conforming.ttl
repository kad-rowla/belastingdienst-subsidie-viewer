# External namespaces
prefix foaf: <http://xmlns.com/foaf/0.1/>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix sdo: <https://schema.org/>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix ex: <https://example.org/>

# Internal namespaces
prefix graph: <https://example.org/graph/>
prefix id: <https://example.org/id/>

  id:ada
    a sdo:Person;

    # sh:DatatypeConstraintComponent
    sdo:birthDate '1800-01-01T01:01:01Z'^^xsd:dateTime;

    # sh:MaxExclusiveConstraintComponent
    # sh:MinExclusiveConstraintComponent
    sdo:deathDate '1700-01-01'^^xsd:date;

    # sh:LanguageInConstraintComponent
    sdo:description
      'monkey'@en,
      'aap'@nl,
      # sh:UniqueLangConstraintComponent
      'noot'@nl;

    # sh:MinCountConstraintComponent
    # No sdo:email

    # sh:HasValueConstraintComponent
    sdo:familyName 'Lovelace';

    # sh:InConstraintComponent
    sdo:givenName 'Ada';

    # sh:ClassConstraintComponent
    sdo:knows id:byron;

    # sh:MaxCountConstraintComponent
    sdo:name
       # sh:MaxLengthConstraintComponent
      'Ada',
       # sh:MinLengthConstraintComponent
       '';

    # sh:NodeKindConstraintComponent
    owl:sameAs 'ada';

    # sh:ClosedConstraintComponent
    foaf:knows id:byron.

  id:byron
    a foaf:Person.


ex:Bob a ex:Person ;
	ex:address ex:BobsAddress .
	
ex:BobsAddress
	ex:postalCode "1234" .

ex:Reto a ex:Person ;
	ex:address ex:RetosAddress .

ex:RetosAddress
	ex:postalCode 5678 .

ex:Bob ex:bCode "b101" .
ex:Alice ex:bCode "B102" .
ex:Carol ex:bCode "C103" .