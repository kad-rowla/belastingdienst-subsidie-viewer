@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix shp: <https://triplydb.com/Triply/sh/model/shp/>.

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix def: <https://triplydb.com/Triply/sh/model/def/>.

shp:nodeShape
  sh:target shp:target ;
  sh:property [
    sh:hasValue def:A ;
    sh:path rdf:type 
  ] .

shp:target
  a sh:SPARQLTarget ;
  sh:select """
    PREFIX dct: <http://purl.org/dc/terms/> 
    PREFIX def: <https://triplydb.com/Triply/sh/model/def/> 
    SELECT ?this { 
      ?this dct:subject def:B . 
    }
    """ .
