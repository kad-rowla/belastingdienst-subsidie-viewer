prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix sh: <http://www.w3.org/ns/shacl#>
prefix shp: <https://triplydb.com/Triply/sh/model/shp/>

prefix def: <https://triplydb.com/Triply/sh/model/def/>

# Property shapes, for class violation
shp:nodeShape
  sh:targetClass def:A;
  sh:xone
    ( [ sh:property [ sh:hasValue def:A;
                      sh:path rdf:type ]]
      [ sh:property [ sh:hasValue def:B;
                      sh:path rdf:type ]] ).

# Duplicate shape violation to test shape filtering.
shp:shapeShouldNeverBeMatched
  sh:targetClass def:C;
  sh:xone
    ( [ sh:class def:C ]
      [ sh:class def:D ] ).
