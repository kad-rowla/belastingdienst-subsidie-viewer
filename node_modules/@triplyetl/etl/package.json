  "bin": {
    "etl": "lib/runner/cli.js",
    "prefixes": "lib/bin/prefixes-from-config.js",
    "tools": "lib/bin/scripts/cli.js"
  },
  "scripts": {
    "build": "rm -rf ./lib && tsc -p ./tsconfig-build.json && chmod +x ./lib/runner/cli.js",
    "build:win": "@powershell -NoProfile -ExecutionPolicy Unrestricted Remove-Item -Recurse -Force lib && npx tsc -p tsconfig-build.json",
    "build:clean": "rm -rf ./lib ./docs",
    "build:clean:win": "@powershell -NoProfile -ExecutionPolicy Unrestricted Remove-Item -Recurse -Force lib",
    "build:wiki": "test -d ../etl.wiki && npx typedoc --plugin typedoc-plugin-markdown --plugin typedoc-gitlab-wiki-theme --out ../etl.wiki && find ../etl.wiki -type f -name '*.md' -exec sed -i '' 's/.md//g' {} ';' && mv ../etl.wiki/home.md ../etl.wiki/TOC.md && mv ../etl.wiki/README.md ../etl.wiki/home.md && git -C ../etl.wiki add . && git -C ../etl.wiki commit -a -m 'Updated docs' && git -C ../etl.wiki push",
    "dev": "npx tsc -p ./tsconfig-build.json --watch --preserveWatchOutput",
    "test": "mocha \"./lib/**/*.test.js\" --timeout 60s --require source-map-support/register",
    "coverage": "nyc mocha \"./lib/**/*.test.js\" --require source-map-support/register",
    "test:ci": "npm run build:clean && npm run util:validateTs && npm run build && npm run util:lint && npx lint-staged && npm run test && ./bin/testCliScripts",
    "preversion": "isCleanBranch",
    "postversion": "versionToBranch",
    "prepublishOnly": "if [ -z \"${CI}\" ]; then echo Publishing is not allowed. Only the CI may publish; exit 1; fi",
    "util:prettify": "prettier --parser typescript --write $(find ./src -name '*.ts')",
    "util:lint": "eslint --ext .tsx --ext .ts ./src",
    "util:validateTs": "tsc -p ./tsconfig-build.json --noEmit --noUnusedParameters",
    "util:validateTs:watch": "tsc -p ./tsconfig-ignore-sandbox.json --noEmit --noUnusedParameters --watch --preserveWatchOutput",
    "_postinstall": "husky install ./bin/hooks",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "debug": "npx etl ./lib/sandbox/example.js"
  },
