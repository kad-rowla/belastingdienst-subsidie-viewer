# Project-specific declarations.
prefix html: <https://example.com/model/html/>
prefix graph: <https://example.com/graph/>
prefix id:    <https://example.com/id/>
prefix txt:    <https://example.com/txt/>


id:R1C1 a html:TH; html:childNodes (txt:H txt:1) .
id:R1C2 a html:TH; html:childNodes (txt:H txt:2) .
id:R1C3 a html:TH; html:childNodes (txt:H txt:3) .

id:R2C1 a html:TD; html:childNodes (txt:A txt:1) .
id:R2C2 a html:TD; html:childNodes (txt:A txt:2) .
id:R2C3 a html:TD; html:childNodes (txt:A txt:3) .

id:R3C1 a html:TD; html:childNodes (txt:B txt:1) .
id:R3C2 a html:TD; html:childNodes (txt:B txt:2) .
id:R3C3 a html:TD; html:childNodes (txt:B txt:3) .

id:R4C1 a html:TD; html:childNodes (txt:C txt:1) .
id:R4C2 a html:TD; html:childNodes (txt:C txt:2) .
id:R4C3 a html:TD; html:childNodes (txt:C txt:3) .

id:R1 a html:TR; html:childNodes (id:R1C1 id:R1C2 id:R1C3) .
id:R2 a html:TR; html:childNodes (id:R2C1 id:R2C2 id:R2C3) .
id:R3 a html:TR; html:childNodes (id:R3C1 id:R3C2 id:R3C3) .
id:R4 a html:TR; html:childNodes (id:R4C1 id:R4C2 id:R4C3) .

id:THEAD a html:THEAD; html:childNodes (id:R1) .
id:TBODY a html:TBODY; html:childNodes (id:R2 id:R3 id:R4) .

id:TABLE a html:TABLE ; html:childNodes (id:THEAD id:TBODY) .
id:BODY a html:BODY; html:childNodes (id:TABLE) .
id:HTML  a html:HTML; html:childNodes (id:BODY) .

txt:A a html:CDATA ; html:fragment "A"^^rdf:HTML .
txt:B a html:CDATA ; html:fragment "B"^^rdf:HTML .
txt:C a html:CDATA ; html:fragment "C"^^rdf:HTML .
txt:H a html:CDATA ; html:fragment "H"^^rdf:HTML .
txt:1 a html:CDATA ; html:fragment "1"^^rdf:HTML .
txt:2 a html:CDATA ; html:fragment "2"^^rdf:HTML .
txt:3 a html:CDATA ; html:fragment "3"^^rdf:HTML .

