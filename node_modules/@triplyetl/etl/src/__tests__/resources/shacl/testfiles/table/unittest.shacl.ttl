@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://ex.com/> .
@prefix id:  <https://example.com/id/> .
@prefix html: <https://example.com/model/html/> .

: a sh:NodeShape ;
  sh:targetSubjectsOf html:fragment ;
  sh:closed true ;
  sh:property [
    sh:path html:fragment ;
    sh:datatype rdf:HTML ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
  ] .

:R1C1 a sh:NodeShape; sh:targetNode id:R1C1; sh:property [ sh:path html:fragment; sh:hasValue  "<th>H 1</th>"^^rdf:HTML ] .
:R1C2 a sh:NodeShape; sh:targetNode id:R1C2; sh:property [ sh:path html:fragment; sh:hasValue  "<th>H 2</th>"^^rdf:HTML].
:R1C3 a sh:NodeShape; sh:targetNode id:R1C3; sh:property [ sh:path html:fragment; sh:hasValue  "<th>H 3</th>"^^rdf:HTML].
:R2C1 a sh:NodeShape; sh:targetNode id:R2C1; sh:property [ sh:path html:fragment; sh:hasValue  "<td>A 1</td>"^^rdf:HTML].
:R2C2 a sh:NodeShape; sh:targetNode id:R2C2; sh:property [ sh:path html:fragment; sh:hasValue  "<td>A 2</td>"^^rdf:HTML].
:R2C3 a sh:NodeShape; sh:targetNode id:R2C3; sh:property [ sh:path html:fragment; sh:hasValue  "<td>A 3</td>"^^rdf:HTML].
:R3C1 a sh:NodeShape; sh:targetNode id:R3C1; sh:property [ sh:path html:fragment; sh:hasValue  "<td>B 1</td>"^^rdf:HTML].
:R3C2 a sh:NodeShape; sh:targetNode id:R3C2; sh:property [ sh:path html:fragment; sh:hasValue  "<td>B 2</td>"^^rdf:HTML].
:R3C3 a sh:NodeShape; sh:targetNode id:R3C3; sh:property [ sh:path html:fragment; sh:hasValue  "<td>B 3</td>"^^rdf:HTML].
:R4C1 a sh:NodeShape; sh:targetNode id:R4C1; sh:property [ sh:path html:fragment; sh:hasValue  "<td>C 1</td>"^^rdf:HTML].
:R4C2 a sh:NodeShape; sh:targetNode id:R4C2; sh:property [ sh:path html:fragment; sh:hasValue  "<td>C 2</td>"^^rdf:HTML].
:R4C3 a sh:NodeShape; sh:targetNode id:R4C3; sh:property [ sh:path html:fragment; sh:hasValue  "<td>C 3</td>"^^rdf:HTML].
:R1 a sh:NodeShape; sh:targetNode id:R1; sh:property [ sh:path html:fragment; sh:hasValue  "<tr><th>H 1</th> <th>H 2</th> <th>H 3</th></tr>"^^rdf:HTML].
:R2 a sh:NodeShape; sh:targetNode id:R2; sh:property [ sh:path html:fragment; sh:hasValue  "<tr><td>A 1</td> <td>A 2</td> <td>A 3</td></tr>"^^rdf:HTML].
:R3 a sh:NodeShape; sh:targetNode id:R3; sh:property [ sh:path html:fragment; sh:hasValue  "<tr><td>B 1</td> <td>B 2</td> <td>B 3</td></tr>"^^rdf:HTML].
:R4 a sh:NodeShape; sh:targetNode id:R4; sh:property [ sh:path html:fragment; sh:hasValue  "<tr><td>C 1</td> <td>C 2</td> <td>C 3</td></tr>"^^rdf:HTML].
:THEAD a sh:NodeShape; sh:targetNode id:THEAD; sh:property [ sh:path html:fragment; sh:hasValue  "<thead><tr><th>H 1</th> <th>H 2</th> <th>H 3</th></tr></thead>"^^rdf:HTML].
:TBODY a sh:NodeShape; sh:targetNode id:TBODY; sh:property [ sh:path html:fragment; sh:hasValue  "<tbody><tr><td>A 1</td> <td>A 2</td> <td>A 3</td></tr> <tr><td>B 1</td> <td>B 2</td> <td>B 3</td></tr> <tr><td>C 1</td> <td>C 2</td> <td>C 3</td></tr></tbody>"^^rdf:HTML].
:TABLE a sh:NodeShape; sh:targetNode id:TABLE; sh:property [ sh:path html:fragment; sh:hasValue  "<table><thead><tr><th>H 1</th> <th>H 2</th> <th>H 3</th></tr></thead> <tbody><tr><td>A 1</td> <td>A 2</td> <td>A 3</td></tr> <tr><td>B 1</td> <td>B 2</td> <td>B 3</td></tr> <tr><td>C 1</td> <td>C 2</td> <td>C 3</td></tr></tbody></table>"^^rdf:HTML].
:BODY a sh:NodeShape; sh:targetNode id:BODY; sh:property [ sh:path html:fragment; sh:hasValue  "<body><table><thead><tr><th>H 1</th> <th>H 2</th> <th>H 3</th></tr></thead> <tbody><tr><td>A 1</td> <td>A 2</td> <td>A 3</td></tr> <tr><td>B 1</td> <td>B 2</td> <td>B 3</td></tr> <tr><td>C 1</td> <td>C 2</td> <td>C 3</td></tr></tbody></table></body>"^^rdf:HTML].
:HTML a sh:NodeShape; sh:targetNode id:HTML; sh:property [ sh:path html:fragment; sh:hasValue  "<html><body><table><thead><tr><th>H 1</th> <th>H 2</th> <th>H 3</th></tr></thead> <tbody><tr><td>A 1</td> <td>A 2</td> <td>A 3</td></tr> <tr><td>B 1</td> <td>B 2</td> <td>B 3</td></tr> <tr><td>C 1</td> <td>C 2</td> <td>C 3</td></tr></tbody></table></body></html>"^^rdf:HTML].
