{
  "name": "@triply/utils",
  "version": "2.0.4",
  "description": "",
  "license": "UNLICENSED",
  "author": "Triply <info@triply.cc>",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -b tsconfig-build.json && chmod +x ./lib/bin/*.js",
    "build:watch": "../../node_modules/.bin/tsc -b tsconfig-build.json --watch --preserveWatchOutput",
    "test": "mocha ./lib/**/__tests__/*-test.js --require source-map-support/register",
    "preversion": "node ./lib/bin/isCleanBranch.js",
    "postversion": "node ./lib/bin/versionToBranch.js utils",
    "util:clear": "rm -rf ./lib",
    "util:lint": "ESLINT_STRICT=true ESLINT_EXPENSIVE=true ../../node_modules/.bin/eslint --ext .tsx --ext .ts ./src",
    "util:prettify": "../../node_modules/.bin/prettier --parser typescript --write $(find ./src -name '*.ts')",
    "util:validateFull": "yarn run util:validateTs && yarn run util:lint && yarn run build && yarn run test",
    "util:validateTs:watch": "../../node_modules/.bin/tsc -p tsconfig.json --watch --preserveWatchOutput",
    "util:validateTs": "../../node_modules/.bin/tsc -p tsconfig.json"
  },
  "bin": {
    "auditor": "./lib/bin/auditor.js",
    "calverIncrement": "./lib/bin/calverIncrement.js",
    "ensureCalver": "./lib/bin/ensureCalver.js",
    "isCleanBranch": "./lib/bin/isCleanBranch.js",
    "versionToBranch": "./lib/bin/versionToBranch.js"
  },
  "devDependencies": {
    "@triply/utils-private": "1.0.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "dedent": "^0.7.0",
    "mocha": "^10.2.0",
    "utils-workspace": "0.0.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.8.0",
    "@types/jsonld": "^1.5.8",
    "@types/wellknown": "^0.5.4",
    "autocast": "0.0.4",
    "commander": "^10.0.1",
    "deepdash": "^5.3.9",
    "find-up": "6.3.0",
    "fs-extra": "^11.1.1",
    "jsonld": "^8.2.0",
    "lodash-es": "^4.17.21",
    "n3": "^1.16.4",
    "numeral": "^2.0.6",
    "pumpify": "^2.0.1",
    "rdf-string": "^1.6.3",
    "source-map-support": "^0.5.21",
    "sparqljs": "3.6.2",
    "through2": "^4.0.2",
    "ts-essentials": "^9.3.2",
    "url-parse": "^1.5.10",
    "wellknown": "^0.5.0",
    "yaml": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./*": {
      "import": {
        "default": "./lib/*"
      }
    }
  },
  "type": "module"
}